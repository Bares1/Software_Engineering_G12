<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="456.0" prefWidth="709.0" spacing="20.0" stylesheets="@catalogM/style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.PrimaryController">
   <children>
      <AnchorPane prefHeight="504.0" prefWidth="710.0">
         <children>
            <Button fx:id="catalogButton" layoutX="592.0" layoutY="33.0" mnemonicParsing="false" onAction="#catalogController" text="Movie Catalog" />
            <TextField fx:id="searchTextField" layoutX="295.0" layoutY="83.0" prefHeight="26.0" prefWidth="250.0" promptText="Search" />
            <AnchorPane layoutY="85.0" prefHeight="372.0" prefWidth="126.0">
               <children>
                  <Button fx:id="customerPanelBtn" layoutX="6.0" layoutY="130.0" mnemonicParsing="false" onAction="#customerController" prefHeight="26.0" prefWidth="116.0" text="Customer Panel" textFill="#1010ff" />
                  <Button fx:id="employeePanelBtn" layoutX="6.0" layoutY="200.0" mnemonicParsing="false" onAction="#employeeController" prefHeight="26.0" prefWidth="116.0" text="Employee Panel" textFill="#00a606" />
                  <Button fx:id="submitComplaintBtn" layoutX="6.0" layoutY="270.0" mnemonicParsing="false" onAction="#complaintController" prefHeight="26.0" prefWidth="116.0" text="Submit Complaint" textFill="#635300" />
                  <Button fx:id="homeScreenBtn" layoutX="6.0" layoutY="60.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="116.0" text="Home Screen" textFill="RED" />
               </children>
            </AnchorPane>
            <ComboBox fx:id="filterByTypeComboBox" layoutX="345.0" layoutY="123.0" onAction="#chooseType" prefHeight="20.0" prefWidth="150.0" />
            <Label layoutX="170.0" layoutY="5.0" prefHeight="56.0" prefWidth="364.0" text="Welcome to Cinema12" textFill="WHITE">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <ScrollPane layoutX="183.0" layoutY="163.0" prefHeight="270.0" prefWidth="474.0">
               <content>
                  <GridPane fx:id="moveisListGrid" onMouseClicked="#chooseMovie" prefHeight="266.0" prefWidth="470.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                  </GridPane>
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
